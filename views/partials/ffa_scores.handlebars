<div id="freeForAllScores">
	<h1>Player Scores</h1>
	<div class="score-holder">
		{{#each groups}}
		<div class="group-score-row">
			<h4 class="group-score-name">{{id}}</h4>
			{{#each playerScores}}
				<div class="score-row" {{#if ../../winners}}
				{{#if (hasOwn ../../winners ../id)}}
					{{#eq (get ../id ../../winners) _name}}
						style="order:-1!important;"
					{{/eq}}
				{{/if}}
			{{/if}}>
				<div class="score-name">{{_name}}</div>
				{{#if ../../winners}}
					{{#if (hasOwn ../../winners ../id)}}
						{{#eq (get ../id ../../winners) _name}}
							<i class="icon-crown round-winner"></i>
						{{/eq}}
					{{/if}}
				{{/if}}
				<div class="score-value">{{_points}}</div>
			</div>
			{{/each}}
		</div>
		{{/each}}
	</div>
</div>